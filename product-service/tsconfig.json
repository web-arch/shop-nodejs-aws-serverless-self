{	
	"compilerOptions": {
		"lib": ["ESNext"],
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": true,
		"sourceMap": true,
		"target": "ES2020",
		"outDir": "lib",
		"baseUrl": ".",
		"paths": {
			"Src/*": ["src/*"],
		},
	},
	"include": ["src/**/*.ts", "serverless.ts"],
	"exclude": [
		"node_modules/**/*",
		".serverless/**/*",
		".webpack/**/*",
		"_warmup/**/*",
		".vscode/**/*"
	],
}
